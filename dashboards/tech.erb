<% content_for :title do %>Camille's dashboard - Tech<% end %>
<style>
  body {
    background-color: black;
  }
  #container {
    width: 100%;
  }
</style>
<div class="row">
    <div class="col-sm-3">
      <div class="double-height" data-id="releases" data-view="List" data-unordered="true" data-title="Boxen releases" style="background-color:cyan"></div>
    </div>
    <div class="col-sm-3">
      <div data-id="github-forks" data-view="List" data-unordered="true" data-title="Forks to update" style="background-color:#DA81F5"></div>
      <div data-id="travis" data-view="Number" data-title="Website Builds" style="background-color:#013ADF"></div>
    </div>
    <div class="col-sm-3">
      <div data-id="github-issues" data-view="Number" data-title="GitHub Issues" style="background-color:#6E6E6E"></div>
      <div data-id="gemnasium" data-view="Number" data-title="Gemnasium" style="background-color:#FFFF00"></div>
    </div>
    <div class="col-sm-3">
      <div data-id="github-prs" data-view="Number" data-title="Pull requests" style="background-color:#8000FF"></div>
      <div data-id="statuscake" data-view="Number" data-title="Monitoring" style="background-color:#3ADF00"></div>
    </div>
</div>
