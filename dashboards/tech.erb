<% content_for :title do %>Camille's dashboard<% end %>

<div class="gridster">
  <ul>
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="2">
      <div data-id="releases" data-view="List" data-unordered="true" data-title="Boxen releases"></div>
    </li>
    <li data-row="1" data-col="2" data-sizex="1" data-sizey="2">
      <div data-id="forks" data-view="List" data-unordered="true" data-title="Forks to update"></div>
    </li>
    <li data-row="1" data-col="3" data-sizex="1" data-sizey="1">
      <div data-id="github-issues" data-view="Number" data-title="GitHub Issues"></div>
    </li>
    <li data-row="1" data-col="4" data-sizex="1" data-sizey="1">
      <div data-id="pulls" data-view="Number" data-title="Pull requests"></div>
    </li>
    <li data-row="2" data-col="3" data-sizex="1" data-sizey="1">
      <div data-id="gemnasium" data-view="Number" data-title="Gemnasium"></div>
    </li>
    <li data-row="2" data-col="4" data-sizex="1" data-sizey="1">
      <div data-id="statuscake" data-view="Number" data-title="Monitoring"></div>
    </li>
  </ul>
</div>
